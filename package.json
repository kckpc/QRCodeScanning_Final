{
  "name": "qr-checkin-app",
  "version": "1.0.0",
  "description": "QR Code Check-in Application",
  "scripts": {
    "start": "cross-env HTTPS=true SSL_CRT_FILE=cert.pem SSL_KEY_FILE=key.pem HOST=0.0.0.0 REACT_APP_API_URL=https://192.168.0.119:3001/api react-scripts start",
    "start:prod": "serve -s build -l 3000 --ssl-cert cert.pem --ssl-key key.pem",
    "build": "react-scripts build",
