    "cors": "^2.8.5",
    "file-saver": "^2.0.5",
    "jsqr": "^1.4.0",
    "moment-timezone": "^0.5.45",
    "multer": "^1.4.5-lts.1",
    "qr-checkin-app": "file:",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-qr-scanner": "^1.0.0-alpha.11",
    "react-scripts": "5.0.1",
    "selfsigned": "^2.1.1",
    "typescript": "^4.9.5",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "concurrently": "^8.2.1",
